    "gulp-sass": "^4.0.1",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-watch": "^5.0.1",
    "nyc": "^13.3.0",
    "require-dir": "^0.3.2",
    "run-sequence": "^2.1.0",
    "sinon": "^3.2.1",
    "tap-nyan": "^1.1.0"
