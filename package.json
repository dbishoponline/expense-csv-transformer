{
  "name": "node-module-boilerplate",
  "version": "1.0.0",
  "description": "A boilerplate for building a new Node.js module",
  "main": "index.js",
  "scripts": {
    "build": "babel --presets es2015,stage-0 -d dist/ src/",
    "test": "ava",
    "debug": "babel --presets es2015,stage-0 -d dist/ src/ && node --inspect-brk ./dist/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dbishoponline/node-module-boilerplate.git"
  },
  "author": "Dylan Bishop",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bluebird": "^3.5.0",
    "crocks": "^0.7.1",
    "fs": "0.0.1-security",
    "ramda": "^0.24.1",
    "ramda-fantasy": "^0.8.0"
  },
  "devDependencies": {
    "ava": "^0.22.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.26.0",
    "debug": "^3.0.1",
    "gulp": "^3.9.1",
    "gulp-jshint": "^2.0.4",
    "gulp-load-plugins": "^1.5.0",
    "gulp-sourcemaps": "^2.6.1",
    "nyc": "^11.2.1",
    "require-dir": "^0.3.2",
    "sinon": "^3.2.1"
  },
  "ava": {
    "files": [
      "dist/*.spec.js",
    ],
    "tap": true,
    "babel": "inherit",
    "require": [
      "babel-register"
    ]
  }
}
