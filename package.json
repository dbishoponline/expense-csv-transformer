  },
  "author": "Dylan Bishop",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "bluebird": "^3.5.0",
    "crocks": "^0.7.1",
    "csv": "^5.1.1",
    "fs": "0.0.1-security",
    "path": "^0.12.7",
    "ramda": "^0.24.1",
    "ramda-fantasy": "^0.8.0"
  },
  "devDependencies": {
    "ava": "^1.3.1",
    "@babel/cli": "^7.1.2",
    "@babel/core": "^7.1.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
